networks:
  backend: {}
  frontend: {}
services:
  eventscatalogapi:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\EventsCatalogAPI
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EventsCatalogAPI
      target: build
    container_name: ecatalogapi
    depends_on:
    - mssqlserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseName: EventDb
      DatabasePassword: ProductApi(!)
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      ExternalEventBaseUrl: http://localhost:6080
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/ecatalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.1/EventsCatalogAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6080
      target: 80
    - target: 80
    volumes:
    - C:\Users\andycom\Documents\GitHub\EventsOnContainers\EventsCatalogAPI:/app:rw
    - C:\Users\andycom\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\andycom\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andycom\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\andycom\.nuget\packages:/root/.nuget/packages:ro
  mssqlserver:
    container_name: eventssqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: ProductApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: {}
    ports:
    - published: 1448
      target: 1433
  tokenserver:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\TokenServiceApi
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: TokenServiceApi
      target: base
    container_name: tokenserviceapi
    depends_on:
    - mssqlserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      MvcClient: http://localhost:5208
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/tokenapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.1/TokenServiceApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6070
      target: 80
    volumes:
    - C:\Users\andycom\Documents\GitHub\EventsOnContainers\TokenServiceApi:/app:rw
    - C:\Users\andycom\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\andycom\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andycom\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\andycom\.nuget\packages:/root/.nuget/packages:ro
  webmvc:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\webmvc
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebMVC
      target: build
    container_name: webshop
    depends_on:
    - eventscatalogapi
    - tokenserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CallBackUrl: http://localhost:5208
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventUrl: http://eventscatalogapi
      IdentityUrl: http://192.168.0.155:6070
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/webmvc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.1/WebMVC.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      frontend: {}
    ports:
    - published: 5208
      target: 80
    volumes:
    - C:\Users\andycom\Documents\GitHub\EventsOnContainers\webmvc:/app:rw
    - C:\Users\andycom\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\andycom\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andycom\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\andycom\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'