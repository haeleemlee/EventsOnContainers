networks:
  backend: {}
  frontend: {}
services:
  basket.data:
    image: redis
    networks:
      backend: {}
    ports:
    - published: 6079
      target: 6379
  cart:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\CartApi
      dockerfile: Dockerfile
    container_name: cartapi
    depends_on:
    - basket.data
    - rabbitmq
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionString: basket.data
      IdentityUrl: http://192.168.0.155:6070
    image: events/cartapi
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6065
      target: 80
  eventscatalogapi:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\EventsCatalogAPI
      dockerfile: Dockerfile
    container_name: ecatalogapi
    depends_on:
    - mssqlserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DatabaseName: EventDb
      DatabasePassword: ProductApi(!)
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      ExternalEventBaseUrl: http://localhost:6080
    image: events/ecatalogapi
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6080
      target: 80
    - target: 80
  mssqlserver:
    container_name: eventssqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: ProductApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: {}
    ports:
    - published: 1448
      target: 1433
  order:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\OrderApi
      dockerfile: Dockerfile
    container_name: orderapi
    depends_on:
    - mssqlserver
    - rabbitmq
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DATABASE: OrdersDb
      IdentityUrl: http://192.168.0.155:6070
      SA_PASSWORD: ProductApi(!)
      SQLSERVER_HOST: mssqlserver
    image: events/orderapi
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6075
      target: 80
  rabbitmq:
    container_name: rabbitmq
    hostname: myrabbitmq
    image: rabbitmq:3-management
    networks:
      backend: {}
    ports:
    - published: 5674
      target: 5671
    - published: 5673
      target: 5672
    - published: 15673
      target: 15672
    volumes:
    - C:\Users\andycom\rabbitmq:/var/lib/rabbitmq/mnesia:rw
  tokenserver:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\TokenServiceApi
      dockerfile: Dockerfile
    container_name: tokenserviceapi
    depends_on:
    - mssqlserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      MvcClient: http://localhost:5208
    image: events/tokenapi
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 6070
      target: 80
  webmvc:
    build:
      context: C:\Users\andycom\Documents\GitHub\EventsOnContainers\webmvc
      dockerfile: Dockerfile
    container_name: webshop
    depends_on:
    - cart
    - eventscatalogapi
    - order
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CallBackUrl: http://localhost:5208
      CartUrl: http://cart
      EventUrl: http://eventscatalogapi
      IdentityUrl: http://192.168.0.155:6070
      OrderUrl: http://order
      StripePrivateKey: sk_test_w6OinVnMa1i8VV1oJDLq4kTA00uoYiDSTs
      StripePublicKey: pk_test_WBsZjG5kBd5zpeS5QDQi8jsm00Wt0yXvfW
    image: events/webmvc
    networks:
      frontend: {}
    ports:
    - published: 5208
      target: 80
version: '3.4'